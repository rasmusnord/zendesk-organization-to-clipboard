{
	"manifest_version": 2,
	"name": "Zendesk Organization to Clipboard",
	"description": "This Chrome Extension makes it easy to copy the organisation label on a ticket in Zendesk to the clipboard.",
	"version": "1.1.0",
	"background":
	{
		"scripts":
		[
			"scripts/background.js"
		]
	},
	"permissions":
	[
		"activeTab",
		"clipboardWrite"
	],
	"browser_action":
	{
		"default_icon": "images/icon.png"
	},
	"options_page": "options.html",
	"commands":
	{
		"_execute_browser_action":
		{
			"description": "Copy to clipboard"
		},
		"search_url_1":
		{
			"description": "Search URL 1"
		},
		"search_url_2":
		{
			"description": "Search URL 2"
		},
		"search_url_3":
		{
			"description": "Search URL 3"
		}
	}
}
